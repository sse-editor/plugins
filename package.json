{
  "name": "@sse-editor/plugins",
  "version": "1.0.0",
  "description": "All the plugins for SSE Editor",
  "main": "./dist/index.umd.js",
  "module": "./dist/index.mjs",
  "types": "./dist/index.d.ts",
  "files": ["dist"],
  "exports": {
    ".": {
      "import": "./dist/index.mjs",
      "require": "./dist/index.umd.js",
      "types": "./dist/index.d.ts"
    },
    "./code": {
      "import": "./dist/code.mjs",
      "require": "./dist/code.umd.js",
      "types": "./dist/code.d.ts"
    },
    "./delimiter": {
      "import": "./dist/delimiter.mjs",
      "require": "./dist/delimiter.umd.js",
      "types": "./dist/delimiter.d.ts"
    },
    "./header": {
      "import": "./dist/header.mjs",
      "require": "./dist/header.umd.js",
      "types": "./dist/header.d.ts"
    },
    "./paragraph": {
      "import": "./dist/paragraph.mjs",
      "require": "./dist/paragraph.umd.js",
      "types": "./dist/paragraph.d.ts"
    },
    "./quote": {
      "import": "./dist/quote.mjs",
      "require": "./dist/quote.umd.js",
      "types": "./dist/quote.d.ts"
    },
    "./warning": {
      "import": "./dist/warning.mjs",
      "require": "./dist/warning.umd.js",
      "types": "./dist/warning.d.ts"
    }
  },
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "npm-run-all build:*",
    "build:index": "vite build --config ./vite.config.js",
    "build:header": "vite build --config ./vite.header.config.js",
    "build:paragraph": "vite build --config ./vite.paragraph.config.js",
    "build:quote": "vite build --config ./vite.quote.config.js",
    "build:delimiter": "vite build --config ./vite.delimiter.config.js",
    "build:warning": "vite build --config ./vite.warning.config.js",
    "build:code": "vite build --config ./vite.code.config.js"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/sse-editor/plugins"
  },
  "author": "SSE World",
  "license": "MIT",
  "devDependencies": {
    "@codexteam/ajax": "^4.2.0",
    "@sse-editor/dom": "^1.0.0",
    "@sse-editor/icons": "^1.0.2",
    "@sse-editor/types": "^1.0.2",
    "npm-run-all": "^4.1.5",
    "vite": "^6.1.1",
    "vite-plugin-css-injected-by-js": "^3.5.2",
    "vite-plugin-dts": "^4.5.0"
  }
}
