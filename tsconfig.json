{
  "compilerOptions": {
    "target": "ES2017",
    "module": "CommonJS",
    "jsx": "react-jsx",
    "lib": ["DOM", "ES2015", "ES2017"],
    "esModuleInterop": true,
    "forceConsistentCasingInFileNames": true,
    "strict": true,
    "skipLibCheck": true,
    "declaration": true,
    "isolatedModules": true
  },
  "exclude": ["node_modules"]
}
